{
  "name": "@qpa/core",
  "version": "0.1.2",
  "description": "pa cli",
  "keywords": [
    "qpa",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chen56/pa.git"
  },
  "homepage": "https://github.com/chen56",
  "bugs": {
    "url": "https://github.com/chen56/issues"
  },
  "author": "chen56",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "bun": "./src/index.ts"
  },
  "bun": {
    "resolve": {
      "extensions": [
        ".tsx",
        ".ts",
        ".jsx",
        ".js"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bun build",
    "test": "bun test",
    "publish-packages": "bun publish --workspaces --if-present"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "@types/node": "^22.15.3",
    "eslint": "^9.26.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "tencentcloud-sdk-nodejs": "^4.0.973"
  }
}
